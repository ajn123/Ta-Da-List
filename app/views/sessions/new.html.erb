


<h1>Sign up</h1>


<div class="row">
  <div class="col-md-6 col-md-offset-3">
  
    <%= form_for(User.new) do |f| %>


      <div class="form-group">
      
      <%= f.label :email, 'email' %>
      <%= f.text_field :email %>

      
      </div>

      <div class="form-group">
      <%= f.label :password, 'password' %>
      <%= f.password_field :password %>
      
      
      </div>


      <%= f.submit "submit" %>

      <% end %>
  
  </div>
</div>
